{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es5",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "outDir": "./build/",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
